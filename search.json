---
layout: null
---
{% assign docs = site.pages
  | where_exp:"p","p.title"
  | where_exp:"p","p.search != false" %}

[
{% for p in docs %}
  {
    "title": {{ p.title | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "content": {{ p.content | strip_html | strip_newlines | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
